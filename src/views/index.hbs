<div class="col-xs-8">
	<div class="jumbotron">
		<div class="container-fluid">
			<table class="table">
				<thead>
					<tr>
						<th>Nombre</td>
						<th>Apellido</td>
						<th>Edad</td>
						<th>Universidad</td>
					</tr>
				</thead>
				<tbody>
					{{#each people}}
					<tr>
						<td>{{name}}</td>
						<td>{{lastname}}</td>
						<td>{{age}}</td>
						<td>{{university}}</td>
					</tr>
					{{/each}}
				</tbody>		
			</table>

			<a href="/add" class="btn btn-danger">Agregar</a>
		</div>
	</div>
</div>
